<svg version="1.1"
    viewBox="0 0 1000 1000"
    width="100%"
    xmlns="http://www.w3.org/2000/svg">
    <style>
    {% block style %}
      * {
          background: #fdfdfd;
          stroke: #020202;
      }

      .light-path {
          stroke: #5b5b5b;
      }
      @media (prefers-color-scheme: dark){
          * {
              background: #020202;
              stroke: #fdfdfd;
          }

	  .light-path {
              stroke: #ababab;
      	  }
      }
    {% endblock %}
    </style>
    {% set frameSize = 800 %}
    {% block contents %}
        {% block frame %}
            <rect
                x="100"
                y="80"
                width="{{ frameSize }}"
                height="{{ frameSize }}"
                fill="none"
            ></rect>
        {% endblock %}
        <g transform="translate(100, 100)">
            {% block graph %}

            {% endblock %}
        </g>
        <g transform="translate(0, 100)">
	    {% block yticks %}

            {% endblock %}
        </g>
        <g transform="translate(100, {{ 100 + frameSize}})">
            {% block xticks %}

            {% endblock %}
        </g>
    {% endblock %}
</svg>