{% extends 'graphs/graph.svg.twig' %}

{% set measuredData = data["humidity"] %}
{% set vMax = measuredData|yUpperLimit|default(100) %}
{% set vMin = measuredData|yLowerLimit|default(0) %}

{% block graph %}
    {% include 'graphs/_components/plot.svg.twig' %}
{% endblock %}

{% block xticks %}
    {% include 'graphs/_components/xticks.svg.twig' %}
{% endblock %}
{% block yticks %}
    {% include 'graphs/_components/yticks.svg.twig' with { 'unit' : '%' } %}
{% endblock %}
